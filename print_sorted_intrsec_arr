int main()
{
    int arr1[] = {1, 3, 4, 5, 7};
    int n=sizeof(arr1)/sizeof(int);
    int arr2[] = {2, 3, 5, 6};
    int m=sizeof(arr2)/sizeof(int);
    int i=0,j=0;
    while(j<m&&i<n)
    {
        if(arr1[i]==arr2[j])
   
       {
       
        printf("%d",arr1[i]);
        i++;
    }
    else
    {
      i++;
      j++;
    }
    
    }
    
    
}
